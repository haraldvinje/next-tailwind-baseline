{
  "name": "next-tailwind-baseline",
  "version": "0.0.57",
  "private": true,
  "scripts": {
    "styles:watch": "npx tailwindcss -i src/styles/globals.css -o src/styles/output.css --watch",
    "dev": "bun dev",
    "dev:styles": "bun dev & bun run styles:watch",
    "clean": "rm -rf node_modules",
    "format": "prettier --write",
    "lint": "npx eslint",
    "lint:fix": "npx eslint --fix --cache",
    "test:gui": "cypress open",
    "test": "cypress run && cypress run --component",
    "postinstall": "husky install"
  },
  "dependencies": {
    "next": "12.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^18",
    "autoprefixer": "^10.4.8",
    "bun-framework-next": "^12",
    "bun-livereload": "^1.0.3",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react-refresh": "0.10.0",
    "tailwindcss": "^3.1.7",
    "typescript": "latest",
    "webpack": "^5.74.0"
  },
  "module": "index.js",
  "lint-staged": {
    "*.js": "bun lint:fix",
    "*.{js,jsx,ts,tsx}": "bun format"
  }
}
